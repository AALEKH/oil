from typing import IO, Any


class LineReader:
  def readline(self) -> str: ...

class BufLineReader(LineReader):
  def __init__(self, s: str): ...
  def readline(self) -> str: ...

def StdIn() -> LineReader: ...


class File:
  def write(self, s: str) -> None: ...

class Buf(File):
  def write(self, s: str) -> None: ...
  def getvalue(self) -> str: ...

def StdOut() -> File: ...


def log(msg: str, *args: Any) -> None: ...
